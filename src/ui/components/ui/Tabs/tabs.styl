.tabList
  border-bottom: 1px solid #007069;
  padding-left: 0;
  cursor: default;

.tabListItem
  display: inline-block;
  list-style: none;
  padding-bottom: 0.7rem;
  margin-right: 0.75rem;
  &:last-child
    margin-right: 0;

.tabListActive
  border-bottom: solid #007069;
  border-bottom-width: 2px;
  font-weight: 400;
